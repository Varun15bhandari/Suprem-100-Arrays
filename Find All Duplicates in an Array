class Solution {
    public List<Integer> findDuplicates(int[] nums) {
        int n=nums.length;
        int[] hash=new int[n+1];
        List<Integer>list=new ArrayList<>();
        for(int num:nums){
            hash[num]++;
            if(hash[num]>1){
                list.add(num);
            }
        }
        return list;
    }
}
